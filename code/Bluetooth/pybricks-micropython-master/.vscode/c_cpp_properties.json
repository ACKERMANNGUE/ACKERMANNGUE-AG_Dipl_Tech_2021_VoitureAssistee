                "${workspaceFolder}/bricks/movehub",
                "${workspaceFolder}/bricks/movehub/build",
                "${workspaceFolder}/micropython"
            ],
            "defines": [
                "MICROPY_ROM_TEXT_COMPRESSION",
                "STM32F070xB",
                "STM32_H=<stm32f0xx.h>",
                "STM32_HAL_H=<stm32f0xx_hal.h>",
                "USE_FULL_LL_DRIVER",
                "FIXMATH_NO_CTYPE"
            ],
            "compilerPath": "${gcc10ArmNoneEabi}",
            "compilerArgs": [
                "-Wall",
                "-Werror",
                "-nostdlib",
                "-fshort-enums",
                "-mthumb",
                "-mtune=cortex-m0",
                "-mcpu=cortex-m0",
