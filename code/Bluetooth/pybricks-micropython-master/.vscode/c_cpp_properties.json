    "env": {
        "gcc10ArmNoneEabi": "${env:GCC_10_ARM_NONE_EABI_BIN}arm-none-eabi-gcc"
    },
    "configurations": [
        {
            "name": "movehub",
            "includePath": [
                "${workspaceFolder}/micropython/lib/cmsis/inc",
